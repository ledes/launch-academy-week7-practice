<h1>Enter a Manufacturer</h1>

<%= form_for @phone do |f| %>

<%= f.label :manufacturer %>:
<%= f.collection_select :manufacturer_id, Manufacturer.all, :id, :name %><br>

<%= f.label :year %>
<%= f.text_field :year %>

<%= f.label :battery_life %>
<%= f.text_field :battery_life %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.submit "Submit" %>
<% end %>
